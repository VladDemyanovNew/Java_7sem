<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
    <display-name>Lab3</display-name>

    <servlet>
        <servlet-name>Sss</servlet-name>
        <servlet-class>Sss</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Ggg</servlet-name>
        <servlet-class>Ggg</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Sss</servlet-name>
        <url-pattern>/Sss</url-pattern>
        <url-pattern>/Sss/forward</url-pattern>
        <url-pattern>/Sss/servletForward</url-pattern>
        <url-pattern>/Sss/htmlForward</url-pattern>
        <url-pattern>/Sss/doubleForward</url-pattern>
        <url-pattern>/Sss/servletRedirect</url-pattern>
        <url-pattern>/Sss/htmlRedirect</url-pattern>
        <url-pattern>/Sss/doubleRedirect</url-pattern>
        <url-pattern>/Sss/redirect</url-pattern>
        <url-pattern>/Sss/httpClient</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Ggg</servlet-name>
        <url-pattern>/Ggg</url-pattern>
        <url-pattern>/Ggg/doubleForward</url-pattern>
        <url-pattern>/Ggg/httpClientResponse</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
